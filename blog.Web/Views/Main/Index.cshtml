@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    var materialUrl = "https://cdn.jsdelivr.net/npm/@mdi/font@latest/css/materialdesignicons.min.css";
}
<!DOCTYPE html>
<html>
<head>
    <base href="/" />
    <meta charset=utf-8>
    <meta name=viewport content="width=device-width,initial-scale=1">
    <title>Compensation</title>
    <link rel="icon" href="~/favicon.ico">
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Roboto:100,300,400,500,700,900">
    <link asp-append-version="true" href="@materialUrl" rel=stylesheet>
    @*<script asp-append-version="true" type=text/javascript src="~/static/js/app.js"></script>
        <script asp-append-version="true" type=text/javascript src="~/static/js/chunk-vendors.js"></script>*@
    <link asp-append-version="true" href="~/static/css/chunk-vendors.css" rel=stylesheet>
    <link asp-append-version="true" href="~/static/css/app.css" rel=stylesheet>
</head>
<body>
    <div id=app></div>

    @{
        string baseUrl = Url.Content("~");
    }

    <script>
        var API_PREFIX = "@baseUrl";
    </script>

    <script asp-append-version="true" type=text/javascript src="~/static/js/chunk-vendors.js"></script>
    <script asp-append-version="true" type=text/javascript src="~/static/js/app.js"></script>
</body>
</html>